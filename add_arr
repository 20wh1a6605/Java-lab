package roll_6605;
import java.io.*;
import java.util.Scanner;
public class Add_array {
	void addele(int[] a) {
		int count = 0;
		for(int i = 0;i<a.length;i++) {
			count += a[i];
		}
		System.out.println("Sum is "+(count));
	}
	public static void main(String[] args) {
		Add_array a1 = new Add_array();
		Scanner s = new Scanner(System.in);
		int[] a = new int[4];
		System.out.println("Give  4 elements");
		for(int i = 0;i<a.length;i++) {
			a[i] = s.nextInt();
		}
		a1.addele(a);
	}
}
